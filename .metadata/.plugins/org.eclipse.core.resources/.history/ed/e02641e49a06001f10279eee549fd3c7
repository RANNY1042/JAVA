<script>
	function validateForm(form){
		if(from.title.value ==""){
			alert("제목을 입력하세요.");
			form.title.focus();
			return false;
		}
		if(form.oFile.value=""){
			alert("첨부 파일은 필수 입력입니다.");
			return false;
		}
	}

</script>
